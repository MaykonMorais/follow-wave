    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "eslint": "^7.29.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-node-dev": "^1.1.7",
    "typescript": "^4.3.4"
  },
  "dependencies": {
    "@overnightjs/core": "1.7.4",
    "@types/config": "^0.0.39",
